{
"type": "CDLNet",
"model": {
	"adaptive": false,
	"num_filters": 484,
	"filter_size": 7,
	"stride": 2,
	"iters": 40,
	"tau0": 1e-2
},
"paths": {
	"save": "Models/CDLNet-test",
	"ckpt": null
},
"train": {
	"loaders": {
		"batch_size": 10,
		"crop_size": 128,
		"trn_path_list": ["CBSD432"],
		"val_path_list": ["Kodak"],
		"tst_path_list": ["CBSD68"]
	},
	"fit": {
		"epochs": 6000,
		"noise_std": 25,
		"val_freq": 50,
		"save_freq": 5,
		"backtrack_thresh": 2,
		"verbose": true,
		"clip_grad": 5e-2
	},
	"opt": {
		"lr": 3e-4
	},
	"sched": {
		"gamma": 0.95,
		"step_size": 50
	}
}
}
